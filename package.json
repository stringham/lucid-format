{
  "name": "lucid-format",
  "displayName": "Lucid Format",
  "description": "Format TypeScript following Lucid Conventions",
  "version": "1.4.0",
  "publisher": "stringham",
  "engines": {
    "vscode": "^1.1.0"
  },
  "dependencies": {
    "typescript": "3.3.1"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "TypeScript"
  ],
  "main": "./out/src/extension",
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "contributes": {
    "configuration": {}
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^7.0.46",
    "vscode": "^1.1.6"
  },
  "icon": "lucid-format.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stringham/lucid-format"
  }
}
